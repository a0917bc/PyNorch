   "source": [
    "import norch\n",
    "\n",
    "x1 = norch.Tensor([[1, 2], \n",
    "                  [3, 4]], requires_grad=True).to(\"cpu\")\n",
    "\n",
    "x2 = norch.Tensor([[4, 3], \n",
    "                  [2, 1]], requires_grad=True).to(\"cpu\")\n",
    "\n",
    "x3 = x1 @ x2\n",
    "result = x3.sum()\n",
    "result.backward()\n",
    "\n",
    "print(x1.grad)"
   ]
